<resources>

     <style name="Theme.Kmm" parent="@style/Theme.Sherlock.Light">
        <item name="android:actionBarItemBackground">@drawable/selectable_background_kmm</item>
        <item name="android:popupMenuStyle">@style/PopupMenu.Kmm</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Kmm</item>
        <item name="android:actionBarTabStyle">@style/ActionBarTabStyle.Kmm</item>
        <item name="android:actionDropDownStyle">@style/DropDownNav.Kmm</item>
        <item name="android:actionBarStyle">@style/ActionBar.Solid.Kmm</item>
        <item name="android:actionModeBackground">@drawable/cab_background_top_kmm</item>
        <item name="android:actionModeSplitBackground">@drawable/cab_background_bottom_kmm</item>
        <item name="android:actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Kmm</item>
        
        
    </style>

    <style name="ActionBar.Solid.Kmm" parent="@style/Widget.Sherlock.Light.ActionBar.Solid">
        <item name="android:background">@drawable/ab_solid_kmm</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_solid_kmm</item>
        <item name="android:backgroundSplit">@drawable/ab_bottom_solid_kmm</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Kmm</item>
    </style>

    <style name="ActionBar.Transparent.Kmm" parent="@style/Widget.Sherlock.Light.ActionBar">
        <item name="android:background">@drawable/ab_transparent_kmm</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Kmm</item>
    </style>
    
    <!-- this style is only referenced in a Light.DarkActionBar based theme -->
    <style name="Theme.Kmm.Widget" parent="@style/Theme.Sherlock">
        <item name="android:popupMenuStyle">@style/PopupMenu.Kmm</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Kmm</item>
    </style>


</resources>